<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="elsuper.david.com.fragmentos">

    <!--Para picasso
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailActivity" />
        <activity android:name=".SummaryActivity" />
        <activity android:name=".RegisterActivity" android:windowSoftInputMode="stateHidden"></activity>
        <service android:name=".service.ServiceTimer"></service>
    </application>

</manifest>